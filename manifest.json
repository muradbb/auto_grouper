{
  "name":"Auto Grouper",
  "description":"Groups tabs with the same url together",
  "version":"0.1.0",
  "manifest_version":3,
  "icons":{
    "16":"images/img16.png",
    "32":"images/img32.png",
    "48":"images/img48.png",
    "128":"images/img128.png"
  },
  "background":{
    "service_worker":"background.js"
  },
  "action":{
    "default_popup":"popup.html",
    "default_icon": {
      "16":"images/img16.png",
      "32":"images/img32.png",
      "48":"images/img48.png",
      "128":"images/img128.png"
    }
  },
  "permissions":[
    "tabs",
    "windows"
  ]
}
